{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-useless-constructor": "off",
        "no-trailing-spaces": "off",
        "padded-blocks": "off",
        "no-unused-vars": "warn",
        "no-multiple-empty-lines": "warn",
        "indent": "warn"
    }
}
